{
  "manifest_version": 3,
  "name": "Simple Icons Tag Copier",
  "description": "Helper for searching / copying tags of Simple Icons.",
  "version": "1.0",
  "action": {
    "default_popup": "./dist/index.html",
    "default_title": "Simple Icons Tag Copier"
  },
  "icons": {
    "16": "./public/icon-16.png",
    "32": "./public/icon-32.png",
    "48": "./public/icon-48.png",
    "128": "./public/icon-128.png"
  }
}
